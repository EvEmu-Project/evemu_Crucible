

<elements>

  <elementDef name="DogmaLM_BindArgs">
    <tupleInline>
      <int name="locationID" />
      <int name="groupID" />
    </tupleInline>
  </elementDef>

  <elementDef name="Notify_OnTarget">
    <tupleInline>
      <!-- setup to go into an OnMultiEvent (notify_type) -->
      <stringInline value="OnTarget" />
      <!--
          One of:
          add - target added
          lost - target lost
          clear - clear all targets
          otheradd - somebody locked you
          otherlost - somebody unlocked you
      -->
      <string name="mode" />
      <int name="targetID" />
      <!-- sent on lost and otherlost, but not processed -->
      <string name="reason" />
    </tupleInline>
  </elementDef>

  <elementDef name="Rsp_Dogma_AddTarget">
    <tupleInline>
      <bool name="flag" />
      <listInt name="targetList" />
    </tupleInline>
  </elementDef>

  <elementDef name="Call_Dogma_Activate">
    <tupleInline>
      <int name="itemID" />
      <wstring name="effectName" safe="true" />
      <!-- optional: -->
      <int name="target" none_marker="0" default="0" />
      <int name="repeat" />
    </tupleInline>
  </elementDef>

  <elementDef name="Call_Dogma_Deactivate">
    <tupleInline>
      <int name="itemID" />
      <wstring name="effectName" safe="true" />
    </tupleInline>
  </elementDef>

  <elementDef name="Call_Dogma_LinkWeapons">
    <tupleInline>
      <int name="shipID" />
      <int name="masterID" />
      <int name="slaveID" />
    </tupleInline>
  </elementDef>

  <elementDef name="Call_Dogma_LoadAmmoToBank">
    <tupleInline>
      <int name="shipID" />
      <int name="masterID" />
      <int name="chargeTypeID" />
      <listInt name="itemIDs" />
      <int name="chargeLocationID" />
      <int name="qty" none_marker="0" safe="true" />
    </tupleInline>
  </elementDef>

  <elementDef name="Call_Dogma_LoadAmmoToModules">
    <tupleInline>
      <int name="shipID" />
      <listInt name="moduleIDs" />
      <int name="chargeTypeID" />
      <int name="itemID" />
      <int name="ammoLocationID" />
    </tupleInline>
  </elementDef>

