                        [PyInt 209]
-->

  <elementDef name="GodmaError"><!-- more to this than template has here.  fix later  -->
    <tupleInline>
      <string name="ErrorMsg" />
        <dictInline>
          <dictInlineEntry key="name">
            <string name="name" />
          </dictInlineEntry>
          <dictInlineEntry key="nameID">
            <int name="nameID" />
          </dictInlineEntry>
        </dictInline>
    </tupleInline>
  </elementDef>

  <elementDef name="Notify_OnGodmaFlushLocation">
    <tupleInline>
